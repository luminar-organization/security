{
  "name": "luminar-organization/security",
  "description": "A security component for the Luminar framework.",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "ext-openssl": "*",
    "luminar-organization/http": "dev-main",
    "luminar-organization/core":  "dev-main",
    "luminar-organization/database": "dev-main",
    "ext-dom": "*",
    "ext-libxml": "*"
  },
  "autoload": {
    "psr-4": {
      "Luminar\\Security\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Luminar\\Security\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=11.3.1",
    "ext-pdo": "*",
    "phpstan/phpstan": "2.0.x-dev",
    "vimeo/psalm": "^6.0@dev"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev"
}
